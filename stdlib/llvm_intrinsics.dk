struct str:
    buf: ptr<u8>
    len: u64

def str_to_int(s: str) -> i32:
    return atoi(s.buf)

def get_str_buf(s: str) -> ptr<u8>:
    return s.buf

def print(str_to_print: str):
    puts(str_to_print.buf)

def print_int(i: i32):
    printf("%i\n".buf, i)

def print_uint(u: u32):
    printf("%i\n".buf, u)

def print_f32(f: f32):
    printf("%f\n".buf, f)

def print_f64(f: f64):
    printf("%d\n".buf, f)