def panic(message: str):
    printf("PANIC: ".buf)
    printf(message.buf)
    printf("\n".buf)
    exit(1)

def assert_i32_equals(a: i32, b: i32):
    if a != b:
        printf("assertion failed: %d != %d\n".buf, a, b)
        exit(1)


def assert_eq<T>(a: T, b: T):
    if a != b:
        printf("assertion failed: %d != %d\n".buf, a, b)
        exit(1)